{
  "name": "letstalk",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "flow-bin": "0.53.0",
    "jest-expo": "^21.0.0",
    "react-native-scripts": "^1.7.0",
    "react-native-typescript-transformer": "^1.1.4",
    "react-test-renderer": "16.0.0-beta.5",
    "redux-logger": "^3.0.6",
    "rimraf": "^2.6.2",
    "ts-jest": "^22.4.1",
    "jest": "^22.4.1",
    "tslint": "^5.8.0",
    "typescript": "^2.8.1"
  },
  "main": "./node_modules/react-native-scripts/build/bin/crna-entry.js",
  "scripts": {
    "start": "yarn clean-build-test && react-native-scripts start",
    "eject": "react-native-scripts eject",
    "android": "react-native-scripts android",
    "ios": "react-native-scripts ios",
    "build": "yarn copy-images && ./node_modules/typescript/bin/tsc",
    "lint": "./node_modules/tslint/bin/tslint src/**/*.ts",
    "clean": "rimraf build coverage",
    "build-watch": "yarn copy-images && ./node_modules/typescript/bin/tsc --watch",
    "test": "jest --verbose",
    "test-watch": "jest --watchAll",
    "build-test": "yarn build && yarn test",
    "clean-build-test": "yarn run clean && yarn build && yarn test",
    "copy-images": "copyfiles -f src/img/*.jpg build/dist/img",
    "postinstall": "./scripts/postinstall.sh"
  },
  "jest": {
    "preset": "jest-expo",
    "transform": {
      "^.+\\.jsx?$": "babel-jest",
      "^.+\\.tsx?$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "useBabelrc": true
      }
    },
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|react-clone-referenced-element|expo(nent)?|@expo(nent)?/.*|react-navigation|sentry-expo))"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "android.ts",
      "android.tsx"
    ]
  },
  "dependencies": {
    "@expo/vector-icons": "^6.2.2",
    "@types/expo": "^24.0.4",
    "@types/expo__vector-icons": "^6.2.0",
    "@types/jest": "^21.1.8",
    "@types/react": "^16.0.25",
    "@types/react-native": "^0.50.7",
    "@types/react-native-vector-icons": "^4.4.2",
    "@types/react-navigation": "^1.0.23",
    "@types/react-redux": "^5.0.13",
    "@types/react-test-renderer": "^16.0.0",
    "@types/redux-form": "^7.0.13",
    "@types/redux-logger": "^3.0.5",
    "babel-preset-expo": "^4.0.0",
    "expo": "^22.0.4",
    "fuzzysearch": "^1.0.3",
    "immutable": "^3.8.2",
    "jest-preset-typescript-react-native": "^1.2.0",
    "node-emoji": "^1.8.1",
    "react": "16.0.0-beta.5",
    "react-native": "^0.49.5",
    "react-native-elements": "^0.19.0",
    "react-native-in-app-notification": "^2.2.0",
    "react-native-keychain": "^2.0.0",
    "react-native-redux-toast": "^1.0.3",
    "react-native-simple-picker": "^1.4.3",
    "react-native-vector-icons": "^4.5.0",
    "react-navigation": "^1.1.2",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-form": "^7.2.3",
    "redux-thunk": "^2.2.0",
    "sentry-expo": "^1.7.0"
  }
}
